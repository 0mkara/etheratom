  },
  "bugs": {
    "url": "https://github.com/gmtcreators/atom-ethereum-interface/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "web3": "~0.15.3",
    "atom-message-panel": "~1.2.4",
    "react": "~0.14.8",
    "react-dom": "~0.14.8"
},
  "configSchema": {
      "rpcAddress": {
          "title": "Ethereum instance",
          "description": "Point to local ethereum instance",
